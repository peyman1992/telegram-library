{
  "name": "peyman1992/telegram-library",
  "description": "a route base telegram library for laravel",
  "type": "library",
  "license": "MIT",
  "version": "0.1.17",
  "autoload": {
    "psr-4": {
      "Peyman1992\\TelegramLibrary\\": "src/"
    },
    "files": [
      "functions/functions.php"
    ]
  },
  "authors": [
    {
      "name": "Peyman Mohsenzadeh",
      "email": "peyman_m1992@yahoo.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "irazasyed/telegram-bot-sdk": "^3.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Peyman1992\\TelegramLibrary\\Provider\\TelegramLibraryServiceProvider"
      ],
      "aliases": {
        "TelegramRoute": "Peyman1992\\TelegramLibrary\\Facade\\TelegramRoute"
      }
    }
  }
}
